pyCodeLoadFont = StringTemplate["
font = ff.open('``')
if font.is_cid:
  font.cidFlattenByCMap('``')
# For Adobe Japan1-7, the following line should be set.
# font.encoding = 'Unicode'
font.sfnt_names
"];
pyCodeGetCurves = StringTemplate["
result = {}
for glyph in ``:
    curves = []
    if glyph in font:
      # print(font[glyph])
      for contour in font[glyph].foreground:
        curvesInContour = []
        temp = []
        temp.append([contour[0].x, contour[0].y])
        for point in contour[1:] + contour[0]:
          temp.append([point.x, point.y])
          if point.on_curve:
            curvesInContour.append(temp)
            temp = [[point.x, point.y]]
        curves.append(curvesInContour)
    else:
      # print('Glyph {} not in the font!'.format(glyph))
      pass
    result[glyph] = curves

result
"];


calcBaseline[base_] :=
  (#[[;;2]] -> AssociationThread[First[base] -> #[[3]]]) & /@ Last[base] // Association


drawGlyph[glyphCurves_] :=
  FilledCurve[BezierCurve /@ Join[{First @ #}, Drop[#, 1, 1]] & /@ glyphCurves]


draw[fontPath_, cidmapPath_, glyphRange_, checkIdeoFace_:False] := Module[
  {fontInfo, horiBase, vertBase, ideoFace, curves},

  (* Import FontForge *)
  Echo[#, "FontForge version:"] & @
    pythonEvaluate["import fontforge as ff; ff.version()"];
    
  (* Load Font *)
  Echo[#, "Time:"] & @ First @ AbsoluteTiming[
    fontInfo = pythonEvaluate @ pyCodeLoadFont[fontPath, cidmapPath]];
  Echo[#, "Font info:"] & @ TextGrid @ fontInfo;
  
  (* Get baseline *)
  If[checkIdeoFace,
    horiBase = calcBaseline @ pythonEvaluate["font.horizontalBaseline"];
    vertBase = calcBaseline @ pythonEvaluate["font.verticalBaseline"];
    (*Echo[horiBase, "Horizontal baseline:"];*)
    (*Echo[vertBase, "Vertical baseline:"];*)
    Echo[#, "Ideographic character face:"] & @
      (ideoFace = Transpose[#[{"hani", "ideo"}] /@ {"icfb", "icft"} & /@ {vertBase, horiBase}]),
    ideoFace = {}
  ];

  (* Draw glyphs *)
  Echo[#, "Time:"] & @ First @ AbsoluteTiming[
    curves = DeleteCases[{}] @ pythonEvaluate @ pyCodeGetCurves @ glyphRange];
  Echo[#, "Size:"] & @ Length[curves];
  
  (* Return *)
  <|
    "Font"     -> pythonEvaluate["font.fullname"],
    "Curves"   -> Values[drawGlyph /@ curves],
    "IdeoFace" -> ideoFace
  |>
]


exportPNG[name_, image_] := Export[name <> ".png", image, Background -> None(*, CompressionLevel \[Rule] 1*)]


